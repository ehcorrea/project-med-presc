{
  "expo": {
    "name": "Med Presc",
    "slug": "project-med-presc",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/splash-icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ehcorrea.projectmedpresc"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/splash-icon.png",
        "backgroundColor": "#2196F3"
      },
      "package": "com.ehcorrea.projectmedpresc",
      "googleServicesFile": "./google/google-services.json"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-secure-store",
      [
        "expo-build-properties",
        {
          "android": {
            "extraMavenRepos": [
              "../../node_modules/@notifee/react-native/android/libs"
            ]
          },
          "ios": {}
        }
      ],
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#2196F3",
          "image": "./src/assets/images/splash-icon.png",
          "imageWidth": 200
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "00079e76-60bb-4916-8645-2f837f24e232"
      }
    },
    "owner": "zctechs"
  }
}
